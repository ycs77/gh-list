{
  "name": "gh-list",
  "type": "module",
  "version": "0.0.1",
  "description": "Categorizing starred repositories on GitHub",
  "license": "MIT",
  "homepage": "https://github.com/ycs77/gh-list#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/gh-list.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "tsx test/gh-list.ts",
    "code-check": "npm run lint && npm run typecheck",
    "prepack": "npm run build",
    "release": "bumpp --commit \"Release v%s\" && npm publish"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "ofetch": "^1.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.17.2",
    "@ycs77/eslint-config": "^5.0.1",
    "bumpp": "^10.2.3",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "tsdown": "^0.15.0",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2"
  }
}
